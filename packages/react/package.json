{
  "name": "@react-pixel-ui/react",
  "version": "1.0.2",
  "description": "React hooks for pixelating CSS styles",
  "keywords": [
    "react",
    "pixel",
    "pixelated",
    "css",
    "canvas",
    "ui",
    "hooks",
    "graphics"
  ],
  "homepage": "https://github.com/Todari/react-pixel-ui#readme",
  "repository": {
